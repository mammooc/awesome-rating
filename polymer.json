{
  "entrypoint": "flexible-rating.js",
  "sources": [
    "src/**/*",
    "package.json"
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ],
    "ignoreWarnings": [
      "unknown-superclass",
      "set-unknown-attribute",
      "databinding-calls-must-be-functions",
      "databind-with-unknown-property"
    ]
  },
  "builds": [
    {
      "name": "es6",
      "bundle": true,
      "js": {"minify": true},
      "css": {"minify": true},
      "html": {"minify": true}
    },
    {
      "name": "es5",
      "bundle": true,
      "js": {"minify": true, "compile": true},
      "css": {"minify": true},
      "html": {"minify": true}
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
