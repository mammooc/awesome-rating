  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@polymer/iron-a11y-keys-behavior": "^3.0.0-pre.21",
    "@polymer/iron-behaviors": "^3.0.0-pre.21",
    "@polymer/iron-form-element-behavior": "^3.0.0-pre.21",
    "@polymer/iron-icon": "^3.0.0-pre.21",
    "@polymer/iron-icons": "^3.0.0-pre.21",
    "@polymer/iron-validatable-behavior": "^3.0.0-pre.21",
    "@polymer/polymer": "^3.0.2"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "^3.0.0-pre.21",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.21",
    "@polymer/iron-test-helpers": "^3.0.0-pre.21",
    "eslint": "^5.2.0",
    "eslint-plugin-html": "^4.0.5",
    "htmlhint": "^0.9.13",
    "polymer-bundler": "^4.0.2",
    "polymer-cli": "^1.7.7",
    "wct-browser-legacy": "^1.0.1",
    "web-component-tester": "^6.7.1"
  },
  "scripts": {
    "test": "node_modules/web-component-tester/bin/wct test/index.html",
    "test-firefox": "node_modules/web-component-tester/bin/wct  test/index.html -l firefox",
    "test-safari": "node_modules/web-component-tester/bin/wct  test/index.html -l safari",
    "test-chrome": "node_modules/web-component-tester/bin/wct  test/index.html -l chrome",
    "testdebugging": "node_modules/web-component-tester/bin/wct -p test/index.html",
    "lint": "eslint src --fix && htmlhint --config .htmlhintrc src/**/*.html index.html && polymer lint",
    "serve": "polymer serve --module-resolution=node --npm",
    "bundle": "polymer build",
    "analyze": "polymer analyze > analysis.json",
    "deploy": "./utils/deploy.sh"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
