    "htmlhint": "^0.9.13",
    "polymer-bundler": "^3.0.0",
    "polymer-cli": "^1.4.0",
    "web-component-tester": "^6.2.0"
  },
  "scripts": {
    "test": "node_modules/web-component-tester/bin/wct test/index.html",
    "test-firefox": "node_modules/web-component-tester/bin/wct  test/index.html -l firefox",
    "test-safari": "node_modules/web-component-tester/bin/wct  test/index.html -l safari",
